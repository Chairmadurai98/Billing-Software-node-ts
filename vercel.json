{
    "version": 2,
    "builds": [
        {
            "src": "public/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/index.js"
        },{
            "src" : "/api/v2/product",
            "dest": "/public/modules/v2/product/product.route.js"
        }
    ]
}