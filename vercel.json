{
    "version": 2,
    "builds": [
        {
            "src": "public/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/public/modules/v2/index.js"
        },
        {
            "src": "/api/v2/product",
            "dest": "/public/modules/v2/product/product.route.js"
        },
        {
            "src": "/api/v2/order",
            "dest": "/public/modules/v2/orders/order.route.js"
        },
        {
            "src": "/api/v2/essential",
            "dest": "/public/modules/v2/essentials/essential.route.js"
        }
    ]
}