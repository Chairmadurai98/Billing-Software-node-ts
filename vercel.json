{
    "version": 2,
    "builds": [
        {
            "src": "public/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/v2/(.*)",
            "dest": "/public/modules/v2/(.*).route.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.js"
        }
    ]
}